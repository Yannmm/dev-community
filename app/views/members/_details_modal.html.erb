<%= form_with model: current_user, url: update_member_details_path, method: :patch do |form| %>
  <div class="modal-body modal-lg">
      <div class="row">
        <div class="col-lg-12">
            <div class="form-group row">
                <%= form.label :first_name, class: "mb-3" %>
                <%= form.text_field :first_name, class: "form-control"%>
                <%= form.label :last_name, class: "mb-3 mt-3" %>
                <%= form.text_field :last_name, class: "form-control"%>
            </div>
        </div>
        <div class="col-lg-12">
            <div class="form-group row">
                <%= form.label :city, class: "mb-3 mt-3" %>
                <%= form.text_field :city, class: "form-control"%>
                <%= form.label :state, class: "mb-3 mt-3" %>
                <%= form.text_field :state, class: "form-control"%>
                <%= form.label :country, class: "mb-3 mt-3" %>
                <%= form.text_field :country, class: "form-control"%>
                <%= form.label :pincode, class: "mb-3 mt-3" %>
                <%= form.text_field :pincode, class: "form-control"%>
                <%= form.label :street_address, class: "mb-3 mt-3" %>
                <%= form.text_field :street_address, class: "form-control"%>
            </div>    
        </div>
        <div class="col-lg-12">
            <class class="form-group row">
                <%= form.label :profile_title, class: "mb-3 mt-3" %>
                <%= form.text_field :profile_title, class: "form-control"%>
            </class>
        </div>
      </div>
  </div>
  <div class="modal-footer">
      <%= form.submit 'Save Changes', class: "btn btn-primary", data: { action: 'click->bs-modal#submitEnd' }%>
      <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
  </div>
<% end %>